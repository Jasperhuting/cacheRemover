{
	"name": "justbrands",
	"displayName": "JustBrands",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.59.0"
	},
	"publisher": "JasperHuting",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:justbrands.removeCacheAcceptance",
		"onCommand:justbrands.removeCacheTest"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "justbrands.removeCacheTest",
				"title": "Remove Cache test"
			},
			{
				"command": "justbrands.removeCacheAcceptance",
				"title": "Remove Cache acceptance"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.59.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
